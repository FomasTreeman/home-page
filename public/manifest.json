{
  "author": "Thomas Freeman",
  "name": "New Tab",
  "manifest_version": 3,
  "version": "1.2",
  "minimum_chrome_version": "103",
  "description": "My custom power user New Tab page.",
  "icons": {
    "16": "favicon.png"
  },
  "permissions": ["bookmarks"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "web_accessible_resources": [
    { "resources": ["index.html"], "use_dynamic_url": true }
  ]
}
